{
  "name": "frontend",
  "private": true,
  "version": "0.9.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev-mode": "cross-env VITE_DEBUG=true vite",
    "build": "npx tsc -b && npx vite build",
    "build:custom": "cross-env ISSUE_REPORT_URL=https://github.com/your-username/mama/issues npx tsc -b && npx vite build",
